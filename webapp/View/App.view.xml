<mvc:View
	controllerName="sap.ui.agi.zeiterfassung.controller.App"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:grid="sap.ui.layout.cssgrid"
	displayBlock="true">
		<grid:CSSGrid gridTemplateColumns="1fr 80%">
			<List
				headerText="Zeiterfassung"
				width="200px">
				<StandardListItem
				 	type="Navigation"
					title="Zeiterfassung"
					press="onNavigateTime"/>
				<StandardListItem
				 	type="Navigation"
					title="Kalender"
					press="onNavigateCalendar"/>
			</List>
			<App id="app"/>
		</grid:CSSGrid>
</mvc:View>